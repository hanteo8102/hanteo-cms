    "color_type": {
      "type": "enumeration",
      "enum": [
        "없음(일반게시물)",
        "파랑(#2f6fd4)",
        "빨강(#ff6666)",
        "검정(#333333)"
      ],
      "default": "nothing"
    },
    "is_delete": {
      "type": "boolean",
      "default": false
    },
